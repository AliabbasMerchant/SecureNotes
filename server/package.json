{
  "name": "secure-notes",
  "version": "1.0.0",
  "description": "A realtime, secure notes and reminders application",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon --exec ts-node src/app.ts",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \" npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AliabbasMerchant/SecureNotes.git"
  },
  "author": "Aliabbas Merchant, Shubhankar Gupta",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/AliabbasMerchant/SecureNotes/issues"
  },
  "homepage": "https://github.com/AliabbasMerchant/SecureNotes#readme",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.11.13",
    "passport": "^0.4.1",
    "socket.io": "^2.4.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.22",
    "@types/passport": "^1.0.5",
    "@types/socket.io": "^2.1.13",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
